{
  "name": "gigabit/affilinet-bundle",
  "description": "A symfony bundle to integrate the affilinet PHP SDK",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Thomas Helmrich",
      "email": "thomas@gigabit.de"
    }
  ],
  "keywords": [
    "affilinet",
    "bundle",
    "symfony"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": "^5.3.9|~7.0",
    "symfony/framework-bundle": "~2.7|~3.0|~4.0",
    "symfony/console": "~2.7|~3.0|~4.0",
    "symfony/dependency-injection": "~2.7|~3.0|~4.0",
    "symfony/http-kernel": "^2.3.24|~3.0|~4.0",
    "gigabit/affilinet-php-sdk": "dev-master"
  },
  "require-dev": {
    "symfony/validator": "~2.7|~3.0|~4.0",
    "symfony/phpunit-bridge": "~2.7|~3.0|~4.0",
    "phpunit/phpunit": "~4"
  },
  "autoload": {
    "psr-0": {
      "Gigabit\\AffilinetBundle": ""
    }
  },
  "target-dir": "Gigabit/AffilinetBundle"
}
